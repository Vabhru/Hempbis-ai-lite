# Server Configuration
PORT=3001
NODE_ENV=development

# Gemini AI
GEMINI_API_KEY=your_gemini_api_key_here

# CORS (comma-separated origins or * for all)
CORS_ORIGIN=*

# Logging
LOG_LEVEL=debug
LOG_TO_FILE=true

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000  # 15 minutes
RATE_LIMIT_MAX=100           # Max requests per window
